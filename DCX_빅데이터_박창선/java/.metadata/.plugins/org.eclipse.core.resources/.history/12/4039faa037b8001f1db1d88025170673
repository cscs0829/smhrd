CREATE TABLE PLAYER (
	ID VARCHAR(50),
	PW VARCHAR(50),
	NAME VARCHAR(50)
);

CREATE TABLE ROOM (
	ROOM_NUM NUMBER,
	ROOM_NAME VARCHAR(50)
);
