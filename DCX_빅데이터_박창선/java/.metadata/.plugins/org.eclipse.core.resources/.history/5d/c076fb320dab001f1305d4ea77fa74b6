import java.util.Scanner;

public class Ex17 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int[] num = new int[10];
		for(int i = 0; i < 10; i++) {
			System.out.print((i+1) + "번 째 정수 입력 >>");
			num[i] = sc.nextInt();
		}
			
		System.out.println("3의 배수 : " );
		for(int i =0; i<10; i++) {
			if(num[i]%3==0) {
				System.out.print(num[i]+ " ");
			}
		}
		}

	}

