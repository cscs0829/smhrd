
public class Ex18 {
	import java.util.Random;
	import java.util.Scanner;
	public static void main(String[] args) {
		Random r = new Random();
        int[] arr = new int[6];
        for (int i = 0; i < arr.length; i++) {
            arr[i] = r.nextInt(45) + 1;
            for (int j = 0; j < i; j++) {
                if (arr[i] == arr[j]) {
                    i--;
                }
            }
        }
        for (int i : arr) {
            System.out.println(" 행운의 숫자 : " + i);
        }
    }
