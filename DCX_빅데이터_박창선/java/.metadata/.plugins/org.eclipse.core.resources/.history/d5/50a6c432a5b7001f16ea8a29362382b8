					System.out.println("ьГБьЮРые╝ ьЧ┤ьЦ┤ы│┤ыЛИ ьЭМьХЕьЭ┤ыЭ╝ъ│а ьаБэЮМ ьЧ┤ьЗаъ░А эХШыВШ ыУдьЦ┤ьЮИыЛд. ьЭМьХЕьЛдыбЬ ъ░Аы│┤ьЮР");
					Thread.sleep(3000);
					sucess = true;

				}
			} else if (num == 2) {
				if (answer == 1) {
					mp3.play(patheffect + "ьГБьЮР.mp3");
					Thread.sleep(2000);
					mp3.play(patheffect + "ьЧ┤ьЗа ьЦ╗ыКФьЖМыжм.mp3");
					Thread.sleep(2000);
					mp3.play(patheffect + "эВды│┤ыУЬ.mp3");
					System.out.println("ьГБьЮРые╝ ьЧ┤ьЦ┤ы│┤ыЛИ ьЭМьХЕьЭ┤ыЭ╝ъ│а ьаБэЮМ ьЧ┤ьЗаъ░А эХШыВШ ыУдьЦ┤ьЮИыЛд. ьЭМьХЕьЛдыбЬ ъ░Аы│┤ьЮР");
					Thread.sleep(3000);
					sucess = true;

				}
			}

		} catch (InterruptedException e) {
			e.printStackTrace();
		}
		return sucess;

	}

